@model CapaPresentacion.ServiceUsuarios.UsuarioBE

@{
    ViewBag.Title = "ELIMINAR USUARIO";
}

<h2>Eliminar</h2>

<h3>Estás seguro que quieres eliminar esto?</h3>
<div>
    <h4>USUARIO</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("ID")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Us_id)
        </dd>

        <dt>
            @Html.DisplayName("NOMBRE")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Us_nombre)
        </dd>

        <dt>
            @Html.DisplayName("FECHA NACIMIENTO")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Us_fech_nacimiento)
        </dd>

        <dt>
            @Html.DisplayName("SEXO")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Us_sexo)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
